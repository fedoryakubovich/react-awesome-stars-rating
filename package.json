{
  "name": "react-awesome-stars-rating",
  "version": "0.12.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint-config-prettier"
    ],
    "plugins": [
      "eslint-plugin-prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "awesome",
    "stars",
    "star",
    "rating",
    "redux",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fedoryakubovich/react-awesome-stars-rating"
  },
  "author": "Fedor Yakubovich",
  "bugs": {
    "url": "https://github.com/fedoryakubovich/react-awesome-stars-rating/issues"
  },
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/lib/**/*.{js,jsx,ts,tsx}",
      "!src/lib/**/*.d.ts"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "upload": "NODE_ENV=production babel src/lib --out-dir dist",
    "prepublishOnly": "npm run upload",
    "clean": "./node_modules/.bin/rimraf build/"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "coveralls": "^3.1.1",
    "enzyme": "^3.11.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "react-redux": "^8.0.2",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "redux-form": "^8.3.8"
  }
}
